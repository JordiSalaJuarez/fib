issn = {10468781},
journal = {Simulation and Gaming},
keywords = {CO2 emissions,UNFCCC,adaptation,climate,climate board game,climate change,climate negotiations,climate politics,climate-change simulation/games,companion modeling,ecological systems,economic development,energy,fossil fuels,game design,global politics,global warming,greenhouse gases,interdisciplinarity,knowledge coproduction,mitigation,natural disasters,renewable energies,role-playing,simulation/games,social networks,sustainability,water management},
