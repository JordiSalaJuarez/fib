abstract = {In this guest editorial, we survey some of the main themes and issues in anthropogenic global warming. We emphasize the great potential of simulation/games as an educational strategy. The diversity of issues in climate change is matched by the variety of simulation/games. We then provide a summary of the main points of each of the eight articles, which together contain a wide range of perspectives on climate change, of types of simulation/gaming, of level of abstraction, and of method of implementation.},
author = {Eisenack, Klaus and Reckien, Diana},
doi = {10.1177/1046878113490568},
